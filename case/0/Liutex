FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Liutex;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    bottom
    {
        type            symmetryPlane;
    }
    hull
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    s0
    {
        type            symmetryPlane;
    }
    s1
    {
        type            symmetryPlane;
    }
    top
    {
        type            symmetryPlane;
    }
}


